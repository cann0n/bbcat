<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdkj.bbcat" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".BbcatApp">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SearchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MedicalOnlineActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.HospitalDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.DoctorListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AskActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CatDortorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.PublishActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>
